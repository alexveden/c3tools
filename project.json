{
  "langrev": "1",
  "warnings":  [ "no-unused" ],
  "dependency-search-paths":  [ "lib" ],
  "dependencies":  [  ],
  "authors":  [ "Alex Veden <i@alexveden.com>" ],
  "version": "0.1.0",
  "sources":  [ "lib/**" ],
  "test-sources":  [ "test/**" ],
  "output": "build",
